<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/btn_color"
                android:dashWidth="15dp"
                android:dashGap="10dp" />
            <solid android:color="#E9FAFF" />
            <corners android:radius="100dp" />
        </shape>

    </item>

    <item
        android:bottom="7dp"
        android:left="7dp"
        android:right="7dp"
        android:top="7dp">
        <shape android:shape="oval">
            <solid android:color="#1F02448A" />
            <corners android:radius="100dp" />
        </shape>

    </item>


    <!-- -->


</layer-list>